{
  "name": "spotify-clone-api",
  "version": "0.1.0",
  "description": "A simple Spotify clone API",
  "main": "src/index.ts",
  "engines": {
    "node": ">=18.20.0"
  },
  "keywords": [
    "cdn",
    "content delivery network",
    "images",
    "performance",
    "security",
    "api",
    "express",
    "node.js",
    "husky",
    "lint-staged",
    "prettier",
    "eslint",
    "rest-api",
    "jwt",
    "nodemon",
    "typescript"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "dev": "npm run swagger && nodemon --watch ./src ./src/index.ts",
    "swagger": "node ./src/swagger.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prepare": "husky",
    "build": "npm run build:swagger && npx tsc",
    "build:swagger": "node ./src/swagger.ts",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write .",
      "eslint . --fix"
    ]
  },
  "dependencies": {
    "@jest/types": "^29.6.3",
    "@prisma/client": "^5.22.0",
    "@types/connect-redis": "^0.0.23",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/pino": "^7.0.5",
    "bcrypt": "^5.1.1",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "fluent-ffmpeg": "^2.1.3",
    "http-errors": "~1.6.3",
    "ioredis": "5.4.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.7",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "redis": "^4.7.0",
    "sharp": "^0.33.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/json-schema": "^7.0.15",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.9.1",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "swagger-autogen": "^2.23.7",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "optionalDependencies": {
    "@img/sharp-linux-arm": "^0.33.5",
    "@img/sharp-win32-x64": "^0.33.5"
  },
  "directories": {
    "doc": "docs"
  },
  "author": "",
  "license": "ISC"
}
